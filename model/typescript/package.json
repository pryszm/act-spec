    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
